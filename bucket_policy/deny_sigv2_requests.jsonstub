{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Test",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::REPLACE/*"
            ],
            "Condition": {
                "StringEquals": {
                    "s3:signatureversion": "AWS"
                }
            }
        }
    ]
}
